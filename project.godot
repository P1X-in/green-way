config_version=4

[application]

config/name="GreenWay - Enchanced Edition"
config/description="Game made on Polish-Swiss Game Jam 2021
This is pos-jam enchanced edition."
run/main_scene="res://scenes/MainMenu.tscn"
config/icon="res://assets/icon.png"
config/windows_native_icon="res://assets/icon.ico"

[autoload]

MouseLayer="*res://scripts/services/mouse_layer.gd"
SimpleAudioLibrary="*res://scripts/services/audio.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

